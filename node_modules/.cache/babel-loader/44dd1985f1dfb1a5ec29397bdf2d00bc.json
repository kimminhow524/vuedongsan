{"ast":null,"code":"import data from \"../src/data.js\";\nimport Discount from \"../src/components/Discount.vue\";\nimport Modal from \"../src/components/Modal.vue\";\nimport Card from \"../src/components/Card.vue\";\nexport default {\n  name: \"App\",\n\n  data() {\n    return {\n      showDiscount: true,\n      back: false,\n      dataOrg: [...data],\n      oclick: 0,\n      data: data,\n      modalOn: false,\n      policeNum: [0, 0, 0],\n      menus: [\"home\", \"product\", \"shop\"]\n    };\n  },\n\n  methods: {\n    sortBack() {\n      this.back = false;\n      this.data = [...this.dataOrg];\n    },\n\n    priceSort() {\n      this.back = true;\n      this.data.sort(function (a, b) {\n        return b.price - a.price;\n      });\n    }\n\n  },\n\n  created() {},\n\n  mounted() {\n    setTimeout(() => {\n      this.showDiscount = false;\n    }, 2000);\n  },\n\n  components: {\n    Discount: Discount,\n    Modal: Modal,\n    Card: Card\n  }\n};","map":{"version":3,"mappings":"AAoBA,OAAOA,IAAP,MAAiB,gBAAjB;AACA,OAAOC,QAAP,MAAqB,gCAArB;AACA,OAAOC,KAAP,MAAkB,6BAAlB;AACA,OAAOC,IAAP,MAAiB,4BAAjB;AAEA,eAAe;AACbC,MAAI,EAAE,KADO;;AAEbJ,MAAI,GAAG;AACL,WAAO;AACLK,kBAAY,EAAC,IADR;AAELC,UAAI,EAAC,KAFA;AAGLC,aAAO,EAAE,CAAC,GAAGP,IAAJ,CAHJ;AAILQ,YAAM,EAAE,CAJH;AAKLR,UAAI,EAAEA,IALD;AAMLS,aAAO,EAAE,KANJ;AAOLC,eAAS,EAAE,CAAC,CAAD,EAAI,CAAJ,EAAO,CAAP,CAPN;AAQLC,WAAK,EAAE,CAAC,MAAD,EAAS,SAAT,EAAoB,MAApB;AARF,KAAP;AAUD,GAbY;;AAcbC,SAAO,EAAE;AACPC,YAAQ,GAAE;AACR,WAAKP,IAAL,GAAU,KAAV;AACA,WAAKN,IAAL,GAAY,CAAC,GAAG,KAAKO,OAAT,CAAZ;AACD,KAJM;;AAKTO,aAAS,GAAE;AACT,WAAKR,IAAL,GAAU,IAAV;AACA,WAAKN,IAAL,CAAUe,IAAV,CAAe,UAASC,CAAT,EAAWC,CAAX,EAAa;AAC1B,eAAOA,CAAC,CAACC,KAAF,GAAQF,CAAC,CAACE,KAAjB;AACD,OAFD;AAGF;;AAVS,GAdI;;AA0BbC,SAAO,GAAE,CAER,CA5BY;;AA8BbC,SAAO,GAAE;AACPC,cAAU,CAAC,MAAI;AACb,WAAKhB,YAAL,GAAkB,KAAlB;AACD,KAFS,EAER,IAFQ,CAAV;AAGD,GAlCY;;AAmCbiB,YAAU,EAAE;AACVrB,YAAQ,EAAEA,QADA;AAEVC,SAAK,EAACA,KAFI;AAGVC,QAAI,EAACA;AAHK;AAnCC,CAAf","names":["data","Discount","Modal","Card","name","showDiscount","back","dataOrg","oclick","modalOn","policeNum","menus","methods","sortBack","priceSort","sort","a","b","price","created","mounted","setTimeout","components"],"sourceRoot":"","sources":["/Users/kimminho/programbase/workspace/vuedongsan/src/App.vue"],"sourcesContent":["<template>\n  <div>\n    <transition name=\"fade\">\n     <Modal @closeModal=\"modalOn=false\" :data=\"data\" :oclick= \"oclick\" :modalOn=\"modalOn\"/>\n    </transition>\n\n  <div class=\"menu\">\n    <a v-for=\"i in menus\" :key=\"i\">{{ i }}</a>\n  </div>\n  <Discount v-if=\"showDiscount==true\"/>\n\n  <button @click=\"priceSort()\">가격순 정렬</button>\n  <button v-if=\"back==true\" @click=\"sortBack()\">되돌리기</button>\n\n  <Card @openModal=\"modalOn=true; oclick= $event\" \n  :data=\"data\" :oclick= \"oclick\" :modalOn=\"modalOn\"/>\n  </div>\n</template>\n\n<script>\nimport data from \"../src/data.js\";\nimport Discount from \"../src/components/Discount.vue\";\nimport Modal from \"../src/components/Modal.vue\";\nimport Card from \"../src/components/Card.vue\";\n\nexport default {\n  name: \"App\",\n  data() {\n    return {\n      showDiscount:true,\n      back:false,\n      dataOrg: [...data],\n      oclick: 0,\n      data: data,\n      modalOn: false,\n      policeNum: [0, 0, 0],\n      menus: [\"home\", \"product\", \"shop\"],\n    };\n  },\n  methods: {\n    sortBack(){\n      this.back=false;\n      this.data = [...this.dataOrg];\n    },\n  priceSort(){\n    this.back=true\n    this.data.sort(function(a,b){\n      return b.price-a.price\n    })\n  }\n  },  \n  created(){\n\n  },\n  \n  mounted(){\n    setTimeout(()=>{\n      this.showDiscount=false;\n    },2000)\n  }, \n  components: {\n    Discount: Discount,\n    Modal:Modal,\n    Card:Card,\n  },\n};\n</script>\n\n<style>\n.fade-enter-from{\n  opacity: 0;\n}\n.fade-enter-active{\n  transition: all 1s;\n}\n.fade-enter-to{\n  opacity: 1;\n}\n.fade-leave-from{\n  opacity: 1;\n}\n.fade-leave-active{\n  transition: all 1s;\n}\n.fade-leave-to{\n  opacity: 0;\n}\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n}\nbody {\n  margin: 0;\n}\n\ndiv {\n  box-sizing: border-box;\n}\n.black-bg {\n  width: 100%;\n  height: 100%;\n  background: rgba(0, 0, 0, 0.5);\n  position: fixed;\n  padding: 20px;\n}\n.white-bg {\n  width: 100%;\n  background: white;\n  border-radius: 8px;\n  padding: 20px;\n}\n.room-img {\n  width: 100%;\n  margin-top: 40px;\n}\n.menu {\n  background: darkslateblue;\n  padding: 15px;\n  border-radius: 5px;\n}\n.menu a {\n  color: white;\n  padding: 10px;\n}\n.disCount {\n  background: white;\n  padding: 10px;\n  margin: 10px;\n  border-radius: 5px;\n}\n\n\n</style>\n"]},"metadata":{},"sourceType":"module"}