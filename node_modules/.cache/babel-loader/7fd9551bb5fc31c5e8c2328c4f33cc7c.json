{"ast":null,"code":"import data from \"../src/data.js\";\nimport Discount from \"../src/components/Discount.vue\";\nimport Modal from \"../src/components/Modal.vue\";\nimport Card from \"../src/components/Card.vue\";\nexport default {\n  name: \"App\",\n\n  data() {\n    return {\n      back: false,\n      dataOrg: [...data],\n      oclick: 0,\n      data: data,\n      modalOn: false,\n      policeNum: [0, 0, 0],\n      menus: [\"home\", \"product\", \"shop\"]\n    };\n  },\n\n  methods: {\n    sortBack() {\n      this.data = [...this.dataOrg];\n      this.back = false;\n    },\n\n    priceSort() {\n      this.back = true;\n      this.data.sort(function (a, b) {\n        return b.price - a.price;\n      });\n    }\n\n  },\n  components: {\n    Discount: Discount,\n    Modal: Modal,\n    Card: Card\n  }\n};","map":{"version":3,"mappings":"AAoBA,OAAOA,IAAP,MAAiB,gBAAjB;AACA,OAAOC,QAAP,MAAqB,gCAArB;AACA,OAAOC,KAAP,MAAkB,6BAAlB;AACA,OAAOC,IAAP,MAAiB,4BAAjB;AAEA,eAAe;AACbC,MAAI,EAAE,KADO;;AAEbJ,MAAI,GAAG;AACL,WAAO;AACLK,UAAI,EAAC,KADA;AAELC,aAAO,EAAE,CAAC,GAAGN,IAAJ,CAFJ;AAGLO,YAAM,EAAE,CAHH;AAILP,UAAI,EAAEA,IAJD;AAKLQ,aAAO,EAAE,KALJ;AAMLC,eAAS,EAAE,CAAC,CAAD,EAAI,CAAJ,EAAO,CAAP,CANN;AAOLC,WAAK,EAAE,CAAC,MAAD,EAAS,SAAT,EAAoB,MAApB;AAPF,KAAP;AASD,GAZY;;AAabC,SAAO,EAAE;AACPC,YAAQ,GAAE;AACR,WAAKZ,IAAL,GAAY,CAAC,GAAG,KAAKM,OAAT,CAAZ;AACA,WAAKD,IAAL,GAAU,KAAV;AACD,KAJM;;AAKTQ,aAAS,GAAE;AACT,WAAKR,IAAL,GAAU,IAAV;AACA,WAAKL,IAAL,CAAUc,IAAV,CAAe,UAASC,CAAT,EAAWC,CAAX,EAAa;AAC1B,eAAOA,CAAC,CAACC,KAAF,GAAQF,CAAC,CAACE,KAAjB;AACD,OAFD;AAGF;;AAVS,GAbI;AA0BbC,YAAU,EAAE;AACVjB,YAAQ,EAAEA,QADA;AAEVC,SAAK,EAACA,KAFI;AAGVC,QAAI,EAACA;AAHK;AA1BC,CAAf","names":["data","Discount","Modal","Card","name","back","dataOrg","oclick","modalOn","policeNum","menus","methods","sortBack","priceSort","sort","a","b","price","components"],"sourceRoot":"","sources":["/Users/kimminho/programbase/workspace/vuedongsan/src/App.vue"],"sourcesContent":["<template>\n  <div>\n    <transition name=\"fade\">\n     